{
  "name": "<%= projectName %>",
  "private": true,
  "version": "0.1.0",
  <%_ if (config.buildTool === 'Vite') { _%>
  "type": "module",
  <%_ } _%>
  "scripts": {
    <%_ if (config.buildTool === 'Vite') { _%>
    "dev": "vite --config config/anton.dev.ts",
    "build": "tsc && vite build --config config/anton.prod.ts",
    "preview": "vite preview",
    <%_ } else { _%>
    "dev": "webpack serve --config config/anton.dev.ts",
    "build": "webpack --config config/anton.prod.ts",
    <%_ } _%>
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,less,scss,html,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.22.3",
    <%_ if (config.stateManagement === 'Redux') { _%>
    "@reduxjs/toolkit": "^2.2.1",
    "react-redux": "^9.1.0"
    <%_ } else if (config.stateManagement === 'Mobx') { _%>
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5"
    <%_ } else if (config.stateManagement === 'Zustand') { _%>
    "zustand": "^4.5.2"
    <%_ } _%>
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    <%_ if (config.buildTool === 'Vite') { _%>
    "@vitejs/plugin-react": "^4.2.0",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "vite": "^5.0.0",
    <%_ } else { _%>
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    <%_ } _%>
    <%_ if (config.cssPreprocessor === 'Less') { _%>
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    <%_ } else if (config.cssPreprocessor === 'Sass') { _%>
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    <%_ } else if (config.cssPreprocessor === 'TailwindCSS') { _%>
    "tailwindcss": "^3.3.5",
    "postcss": "^8.4.31",
    "autoprefixer": "^10.4.16",
    "postcss-loader": "^7.3.3",
    <%_ } _%>
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3"
  }
} 